{
    "manifest_version": 2,
    "name": "Show My History",
    "description": "This extension shows the users history and linking of its pages.",
    "version": "0.0.0.2",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}